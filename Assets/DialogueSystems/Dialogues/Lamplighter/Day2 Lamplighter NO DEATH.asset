        speaker: {fileID: 11400000, guid: 44cf7d3d0f4b54b449c8109b737b7469, type: 2}
        expression: 2
        dialogueText: "\u2018\u2019I found this yesterday night and I remember you
          saying that you had one long ago..\u2019\u2019 "
        Options:
        - ResponseText: Continue
          index: 0
    - rid: 2586427488681853496
      type: {class: TextNodeData, ns: DialogueSystem, asm: Assembly-CSharp}
      data:
