        speaker: {fileID: 11400000, guid: 44cf7d3d0f4b54b449c8109b737b7469, type: 2}
        expression: 1
        dialogueText: 'Well.. At least my night adventure resulted in something good '
        Options:
        - ResponseText: Continue
          index: 0
    - rid: 2586427488681853494
