        speaker: {fileID: 11400000, guid: 44cf7d3d0f4b54b449c8109b737b7469, type: 2}
        expression: 0
        dialogueText: "I don't think I should tell him. Not letting this spread across
          the town is the right move\_"
        Options:
        - ResponseText: Continue
          index: 0
    - rid: 2586427461972787335
      type: {class: TextNodeData, ns: DialogueSystem, asm: Assembly-CSharp}
