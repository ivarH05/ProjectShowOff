        speaker: {fileID: 11400000, guid: d49f226d1e23bd048b3a52790017aeaa, type: 2}
        expression: 0
        dialogueText: Sit down, I will be with you soon. Just finishing up this last
          thing for Pieterzoon then I'll be over
        Options:
        - ResponseText: Continue
          index: 0
    - rid: 2586427461972787322
      type: {class: TextNodeData, ns: DialogueSystem, asm: Assembly-CSharp}
