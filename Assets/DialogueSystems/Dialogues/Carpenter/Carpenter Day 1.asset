        speaker: {fileID: 11400000, guid: 44cf7d3d0f4b54b449c8109b737b7469, type: 2}
        expression: 0
        dialogueText: "\"Thank you, I have a feeling my dad would kill me if he knew\u201D"
        Options:
        - ResponseText: Continue
          index: 0
