        speaker: {fileID: 11400000, guid: 44cf7d3d0f4b54b449c8109b737b7469, type: 2}
        expression: 0
        dialogueText: '"But the paper can still be lost or burned, can it not?"'
        Options:
        - ResponseText: Continue
          index: 0
    - rid: 2736092980574421388
