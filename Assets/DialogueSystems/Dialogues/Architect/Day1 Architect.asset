        speaker: {fileID: 11400000, guid: 44cf7d3d0f4b54b449c8109b737b7469, type: 2}
        dialogueText: 'A frail voice reaches out before inevitably tripping as I
          bump into her. I catch her by her arm, but several metal sounds follow
          against the pebbles on the ground. '
        Options:
        - ResponseText: Continue
          index: 0
    - rid: 5474676788425916955
      type: {class: TextNodeData, ns: DialogueSystem, asm: Assembly-CSharp}
      data:
