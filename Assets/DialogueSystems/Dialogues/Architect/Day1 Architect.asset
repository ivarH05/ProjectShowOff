        speaker: {fileID: 11400000, guid: 44cf7d3d0f4b54b449c8109b737b7469, type: 2}
        dialogueText: 'The girl got up from my grip, adjusting her golden locks and
          beaten-up glasses, shooting me an angry look, before looking away when
          aware of their intent. '
        Options:
        - ResponseText: Continue
          index: 0
    - rid: 5474676788425916957
      type: {class: TextNodeData, ns: DialogueSystem, asm: Assembly-CSharp}
      data:
