  references:
    version: 2
    RefIds:
    - rid: 2586427488681853443
      type: {class: NodeData, ns: DialogueSystem, asm: Assembly-CSharp}
      data:
        GUID: dbd8abc5-c39f-47da-8ee6-bcac30816933
        type: 3
